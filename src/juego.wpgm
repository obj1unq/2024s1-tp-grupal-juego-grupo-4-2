import wollok.game.*
import froggi.*
import Frutas.*

program juego {
	game.width(20)
	game.height(10)
	game.boardGround("calles.png")
	game.title("Juego Base")
	game.addVisualCharacter(froggi)
	keyboard.c().onPressDo({ froggi.actualizarEstado() })
	game.onTick(3000, "CREAR FRUTA", { 
		frutasManager.crearFruta()
	game.onCollideDo(froggi,{objeto => objeto.colision(froggi) })
	game.onTick(3000, "CREAR COMIDA", { 
		manzanaFactory.crearComida()
		})
	game.start()
}